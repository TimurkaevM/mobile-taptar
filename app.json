{
  "expo": {
    "name": "Taptar",
    "slug": "teptar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "https",
    "icon": "./assets/icon-Ios.png",
    "splash": {
      "image": "./assets/splash-teptar.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timurkaev.teptar",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-android.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.timurkaev.teptar",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
